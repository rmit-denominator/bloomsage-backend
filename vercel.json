{
    "version": 2,
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/main.py"
        }
    ],
    "env": {
        "APP_MODULE": "main:app"
    }
}